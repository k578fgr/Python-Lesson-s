def get_together_games(games_1, games_2):
    together_games = set(games_1).intersection(set(games_2))
    return together_games

anfisa_games = [
    'Online-chess',
    '–ì–æ—Ä–æ–¥–∞',
    'DOOM',
    '–ö—Ä–µ—Å—Ç–∏–∫–∏-–Ω–æ–ª–∏–∫–∏'
]
alisa_games = [
    'DOOM',
    'Online-chess',
    '–ì–æ—Ä–æ–¥–∞',
    'GTA',
    'World of tanks'
]


together_games = get_together_games(anfisa_games, alisa_games)

for game in together_games:
    print("üëæ", game)

# –í—ã–≤–æ–¥–∏—Ç —Ç–æ–∂–µ —Å–∞–º–æ–µ, –Ω–æ —Å—á–∏—Ç–∞–µ—Ç—Å—è –Ω–µ –≤–µ—Ä–Ω—ã–º

–í–∞—Ä–∏–∞–Ω—Ç 3

---
def get_together_games(games_1, games_2):
    # –ù–∞–ø–∏—à–∏—Ç–µ –∑–¥–µ—Å—å –∫–æ–¥ —Ñ—É–Ω–∫—Ü–∏–∏ –¥–ª—è –ø–æ–∏—Å–∫–∞ –ø–µ—Ä–µ—Å–µ—á–µ–Ω–∏–π
    return together_games
anfisa_games = [
    'Online-chess',
    '–ì–æ—Ä–æ–¥–∞',
    'DOOM',
    '–ö—Ä–µ—Å—Ç–∏–∫–∏-–Ω–æ–ª–∏–∫–∏'
]
alisa_games = [
    'DOOM',
    'Online-chess',
    '–ì–æ—Ä–æ–¥–∞',
    'GTA',
    'World of tanks'
]
# –í—ã–∑–æ–≤–∏—Ç–µ —Ñ—É–Ω–∫—Ü–∏—é —Å–æ —Å–ø–∏—Å–∫–∞–º–∏ –∏–≥—Ä –≤ –∫–∞—á–µ—Å—Ç–≤–µ –ø–∞—Ä–∞–º–µ—Ç—Ä–æ–≤
together_games = set(alisa_games).intersection(set(anfisa_games))
# –ù–∞–ø–µ—á–∞—Ç–∞–π—Ç–µ –∏—Ç–æ–≥–æ–≤—ã–π –ø–µ—Ä–µ—á–µ–Ω—å –∏–≥—Ä –≤ —Ü–∏–∫–ª–µ
for game in together_games:
    print('üëæ', game)