def get_together_games(anfisa_games, alisa_games):
    return set(anfisa_games).intersection(set(alisa_games))

anfisa_games = [
    'Online-chess',
    '–ì–æ—Ä–æ–¥–∞',
    'DOOM',
    '–ö—Ä–µ—Å—Ç–∏–∫–∏-–Ω–æ–ª–∏–∫–∏'
]
alisa_games = [
    'DOOM',
    'Online-chess',
    '–ì–æ—Ä–æ–¥–∞',
    'GTA',
    'World of tanks'
]

together_games = get_together_games(anfisa_games, alisa_games)

for game in together_games:
    print('üëæ', game)

–†–µ–∑—É–ª—å—Ç–∞—Ç

üëæ –ì–æ—Ä–æ–¥–∞
üëæ DOOM
üëæ Online-chess

–ë–æ–ª–µ–µ –≤–µ—Ä–Ω—ã–π –≤–∞—Ä–∏–∞–Ω—Ç
–í–∞—Ä–∏–∞–Ω—Ç 2

